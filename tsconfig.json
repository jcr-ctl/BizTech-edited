{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2016",
    "strict": false,
    "lib": ["esnext", "dom"],
    "types": ["react", "react-native"]
  },
  "include": [
    "app/**/*",
    "assets/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
